Import("genv")
env1 = genv.Clone()
lib1=genv.SharedLibrary("dataio",Glob("src/*.cpp"))
env1.Append(LIBS=["boost_program_options","dataio"])
env1.ParseConfig("root-config --cflags --glibs")
env1.Append(LINKFLAGS='-Wl,--no-as-needed')
exe2=env1.Program("ParseTree", ["ParseTree.cpp"])
Install("..",lib1)
Install("..",exe2)
Return("lib1")
